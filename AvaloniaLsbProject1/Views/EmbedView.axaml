<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaloniaLsbProject1.Views.EmbedView"
			 xmlns:vm="clr-namespace:AvaloniaLsbProject1.ViewModels"
			  x:DataType="vm:EmbedViewModel">
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Embed Message in Video Screen"/>
		<!-- Additional UI elements for embedding will go here -->
		<Button Content="Select Video" Command="{Binding SelectVideoCommand}" />
		<Button Name="BackButton" Content="Back to Home" />
		<TextBox Text="{Binding SelectedVideoPath}" Width="300" Margin="5" IsReadOnly="True" />
		<Button Content="Embedd measge in video" Command="{Binding EmbeddMessageCommand}" />
	</StackPanel>
</UserControl>
